<!DOCTYPE html>
<html lang="en" dir="ltr">

<head>
  <%- include('./partials/head.ejs')%>
</head>

<body>
  <nav>
    <a href="/products/new">Add a New Product</a>
  </nav>
  <h1>Products index page</h1>
  <ul>
    <% products.forEach(product=> { %>
      <li>
        <a href="/products/<%=product._id %>">
          <%=product.name; %>
        </a>
      </li>
      <% }) %>
  </ul>
</body>

</html>